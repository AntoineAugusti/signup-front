{
  "name": "api-particulier-contractualisation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:scripts",
    "lint:scripts": "xo"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "cross-env": "^5.1.3",
    "next": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-xo-nextjs": "^1.2.0",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.2.1",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "xo": "^0.20.2"
  },
  "xo": {
    "extends": "xo-nextjs",
    "semicolon": false,
    "space": 2,
    "env": "browser",
    "rules": {
      "camelcase": "warn",
      "react/jsx-uses-vars": [2],
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "@env"
          ]
        }
      ]
    },
    "overrides": [
    	{
    		"files": "**/tests/**/*.test.js",
    		"extends": [
    			"plugin:jest/recommended"
    		],
    		"plugins": [
    			"jest"
    		],
    		"env": {
    			"est/globals": true
    		}
    	}
    ]
  }
}
