{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        ["dotenv-import", {
          "moduleName": "@env"
        }],
        [
          "inline-import",
          {
            "extensions": [".css"]
          }
        ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        ["dotenv-import", {
          "moduleName": "@env"
        }],
        [
          "inline-import",
          {
            "extensions": [".css"]
          }
        ]
      ]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]],
      "plugins": [
        ["dotenv-import", {
          "moduleName": "@env"
        }],
        [
          "inline-import",
          {
            "extensions": [".css"]
          }
        ]
      ]
    }
  }
}
